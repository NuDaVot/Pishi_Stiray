<Window x:Class="Mvvm_test1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Mvvm_test1"
        xmlns:vm="clr-namespace:Mvvm_test1.ViewModel"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Window.DataContext>
        <vm:MainWimdowViewModel></vm:MainWimdowViewModel>
    </Window.DataContext>
    <Grid>
        <Frame NavigationUIVisibility="Hidden" Source="{Binding NamePage}" ></Frame>
    </Grid>
</Window>
